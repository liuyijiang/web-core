<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="visitor" namespace="/" extends="struts-default">
        
        <global-results>
		    <result name="error">/WEB-INF/jsp/main/error.jsp</result>
	     </global-results>
		<global-exception-mappings>
			<exception-mapping result="error" exception="java.lang.Exception" />
		</global-exception-mappings>
        
        <action name="vistiorShowSubjectExtra" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="metooVistiorShowSubjectExtraView">
             <result name="success" >/WEB-INF/jsp/visitor/visitor_subject_extra_info.jsp</result>
         </action>
        
        <action name="visitorSeePartsExtraInfo" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="metooVisitorSeePartsExtraInfoView">
             <result name="success" >/WEB-INF/jsp/visitor/visitor_parts_extra_info.jsp</result>
         </action>
        
        <action name="visitorSeeUserShareSingleParts" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorSeeUserShareSinglePartsView">
             <result name="success" >/WEB-INF/jsp/visitor/vistior_user_share_parts_index.jsp</result>
         </action>
        
         <action name="visitorShowSinglePartsComments" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowSinglePartsCommentsView">
             <result name="success" >/WEB-INF/jsp/visitor/visitor_single_part_commnets.jsp</result>
         </action>
        
         <action name="visitorShowPartsCollecter" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowPartsCollecterView">
             <result name="success" >/WEB-INF/jsp/visitor/visitor_parts_collecion_view.jsp</result>
         </action>
        
        <action name="visitorShowPartSilderView" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowPartSilderView">
             <result name="success" >/WEB-INF/jsp/visitor/visitor_part_silder.jsp</result>
        </action>
        
        <action name="visitiorShowSubjectComements" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitiorShowSubjectComementsView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_subject_comments.jsp</result>
<!--             <result name="TEXT" >/WEB-INF/jsp/visitor/visitor_subject_comments_text.jsp</result> -->
<!--             <result name="WAV" >/WEB-INF/jsp/visitor/visitor_subject_comments_wav.jsp</result> -->
        </action>
        
        <action name="visitiorShowSubjectDashBorad" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitiorShowSubjectDashBoradView">
             <result name="success" >/WEB-INF/jsp/visitor/visitor_subject_dash_board.jsp</result>
        </action>
        
        <action name="partDashBoard" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowPartDashBoardView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_part_dash_board.jsp</result>
        </action>
        
        <action name="visitorShowPartDetail" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowPartDetailView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_part_detail.jsp</result>
        </action>
        
         <action name="visitorShowPartDetailPop" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowPartDetailViewPop">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_part_detail_pop.jsp</result>
        </action>
        
        <action name="visitorShowPartDetailFromShare" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowPartDetailFromShareView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_part_detail.jsp</result>
        </action>
        
        <action name="vistiorShowUserIndex" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVistiorShowUserIndexView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_user_subject_index.jsp</result>
        </action>
        
         <action name="vistiorShowSubjectDatail" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVistiorShowSubjectDatailView">
            <result name="PUBLIC" >/WEB-INF/jsp/visitor/visitor_public_subject_detail.jsp</result>
            <result name="FOR-ALL" >/WEB-INF/jsp/visitor/visitor_for_all_subject_detail.jsp</result>
        </action>
        
         <action name="vistiorShowSubjectDatailFromShare" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVistiorShowSubjectDatailFromShareView">
            <result name="PUBLIC" >/WEB-INF/jsp/visitor/visitor_public_subject_detail.jsp</result>
            <result name="FOR-ALL" >/WEB-INF/jsp/visitor/visitor_for_all_subject_detail.jsp</result>
        </action>
        
        <action name="visitorShowJoinSubjectUsers" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowJoinSubjectUsersView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_join_subject_user.jsp</result>
        </action>
        
        <action name="visitorShowSubjectMaterial" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowSubjectMaterialView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_subject_material.jsp</result>
        </action>
        
        <action name="visitorShowPartsCommnets" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowPartsCommnetsView">
            <result name="success" >/WEB-INF/jsp/visitor/visitor_part_commnets.jsp</result>
        </action>
        
    </package>

    <package name="visitorajax"  namespace="/" extends="json-default">
    
       <action name="showTopParts" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
            method="mxkVisitorShowTop10PartsView">
              <result type="json" name="success">  
                <param name="root">topRespone</param>
              </result> 
        </action>
    
		<action name="visitorLoadMoreParts" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
			method="mxkVisitorLoadMorePartsAjax">
		    <result type="json" name="success">  
                <param name="root">partShowResponse</param>
            </result> 
		</action>
		
		<action name="visitorSeeUser" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
			method="mxkVisitorSeeUserAjax">
		    <result type="json" name="success">  
                <param name="root">targetUserVO</param>
            </result> 
		</action>
		  
		<action name="visitorFilterPartsType" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
			method="mxkVisitorFilterPartsTypeAjax">
		    <result type="json" name="success">  
                <param name="root">partShowResponse</param>
            </result> 
		</action>
		
	    <action name="visitorLoadMoreSubject" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
			method="mxkVisitorLoadMoreSubjectAjax">
		    <result type="json" name="success">  
                <param name="root">visitorSearchSubjectRespone</param>
            </result> 
		</action>
		
		<action name="visitorFilterSubjectByType" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
			method="mxkVisitorFilterSubjectByTypeAjax">
		    <result type="json" name="success">  
                <param name="root">visitorSearchSubjectRespone</param>
            </result> 
		</action>
		
		<action name="visitorLoadMoreJoinSubjectUsers" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
			method="mxkVisitorLoadMoreJoinSubjectUsersAjax">
		    <result type="json" name="success">  
                <param name="root">subjectJoinPeopleRespone</param>
            </result> 
		</action>
		
		<action name="loadMoreUserShareSingleParts" class="com.mxk.org.web.visitor.action.MxkVisitorAction"
			method="mxkLoadMoreUserShareSinglePartsAjax">
		    <result type="json" name="success">  
                <param name="root">partShowResponse</param>
            </result> 
		</action>
		
   </package>
   
   
</struts>
